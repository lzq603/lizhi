<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Zhuanzhuan</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>Cookies</filter-name>
    <filter-class>com.zhuanzhuan.filter.CookiesFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Cookies</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>Code</filter-name>
    <filter-class>com.zhuanzhuan.filter.CodeFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Code</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>UserFilter</filter-name>
    <filter-class>com.zhuanzhuan.filter.UserFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>UserFilter</filter-name>
    <url-pattern>/center/*</url-pattern>
    <url-pattern>/ShoppingCarts</url-pattern>
    <url-pattern>/release_info-input.jsp</url-pattern>
    <url-pattern>/confirm_order.jsp</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.zhuanzhuan.action.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Good</servlet-name>
    <servlet-class>com.zhuanzhuan.action.GoodServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Good</servlet-name>
    <url-pattern>/Good</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ValidCode</servlet-name>
    <servlet-class>com.zhuanzhuan.action.ValidCodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidCode</servlet-name>
    <url-pattern>/ValidCode</url-pattern>
    <url-pattern>/center/ValidCode</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Messages</servlet-name>
    <servlet-class>com.zhuanzhuan.action.MessageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Messages</servlet-name>
    <url-pattern>/Messages</url-pattern>
    <url-pattern>/center/Messages</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteMsg</servlet-name>
    <servlet-class>com.zhuanzhuan.action.DeleteMsgServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteMsg</servlet-name>
    <url-pattern>/DeleteMsg</url-pattern>
    <url-pattern>/center/DeleteMsg</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddGood</servlet-name>
    <servlet-class>com.zhuanzhuan.action.AddGoodServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddGood</servlet-name>
    <url-pattern>/AddGood</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Identify</servlet-name>
    <servlet-class>com.zhuanzhuan.action.IdentifyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Identify</servlet-name>
    <url-pattern>/Identify</url-pattern>
  </servlet-mapping>
</web-app>